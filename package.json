{
    "name": "code-breeze",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "bash serve.sh",
        "docker": "npm run docker:stop && docker-compose up -d",
        "docker:start": "npm run docker:stop && docker-compose up --build -d",
        "docker:stop": "docker-compose down --remove-orphans",
        "docker:log": "docker logs -f code-breeze",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "pm2:start": "pm2 start npm --name code-breeze -- start",
        "pm2:restart": "pm2 restart code-breeze",
        "pm2:stop": "pm2 delete code-breeze",
        "lint": "next lint"
    },
    "dependencies": {
        "@radix-ui/react-icons": "^1.3.0",
        "@types/leaflet": "^1.9.12",
        "classnames": "^2.5.1",
        "css-loader": "^7.1.2",
        "emailjs-com": "^3.2.0",
        "leaflet": "^1.9.4",
        "next": "14.2.14",
        "nodemailer": "^6.9.15",
        "react": "^18",
        "react-dom": "^18",
        "react-intersection-observer": "^9.13.1",
        "react-leaflet": "^4.2.1",
        "sass": "^1.79.4",
        "sass-loader": "^16.0.2"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/nodemailer": "^6.4.16",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "^8",
        "eslint-config-next": "14.2.14",
        "mini-css-extract-plugin": "^2.9.1",
        "style-loader": "^4.0.0",
        "typescript": "^5"
    }
}